@page
@model UsuariosModel
@{
    ViewData["Title"] = "Usuarios";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title> Usuarios </title>
    <style>
        .container-repos {
            background: #CdCdCd;
            border: 1px solid #666;
            color: #000;
            width: 100%;
            height: auto;
        }

        .datos {
            display: flex;
            flex-direction: row;
            align-items: center;
            justify-content: space-around;
        }
        .usuarios {
            background: #65CAAC;
            color: #ffffff;
            width: 140px;
            height: 50px;
            border-radius: 4px;
            border: transparent;
        }
        .titular {
            display: flex;
            flex-direction: row-reverse;
        }
    </style>
</head>
<body>
    <div class="titular"><button class="usuarios" type="submit">USUARIOS</button></div>
    <div class="container-repos">
        <div class="datos">
            <div>ID</div>
            <div>USUARIO</div>
            <div>CLAVE</div>
            <div>ESTADO</div>
            <div>RUT</div>
            <div>NOMBRE</div>
            <div>PERFIL</div>
        </div>
    </div>
</body>
</html>